<h1><%= params[:controller].humanize %> - <%= params[:action].humanize %> - <%= params[:option].humanize %></h1>

<%= form_for(@all_types_object, :url => update_test_class_update_update_path(params[:option]), :method => "put") do |f| %>
  <div class="field">
    <%= label_tag(:id) %><br />
    <%= select_tag(:id, options_for_select(AllTypesObject.order(:id).all.map(&:id)), :multiple => @multi) %>
  </div>
  <%= render 'all_types_objects/fields', :f => f %>
  <div class="actions">
    <%= f.submit "Update #{@name}" %>
  </div>
<% end %>