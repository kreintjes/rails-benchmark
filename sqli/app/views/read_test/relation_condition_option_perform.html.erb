<h1><%= params[:controller].humanize %> - <%= params[:action].humanize %> - <%= params[:apply_method].humanize %> - <%= params[:argument_type].humanize %> - <%= params[:argument_type_option].try(:humanize) %></h1>

<% if @all_types_objects.present? %>
  <% @all_types_objects.each do |all_types_object| %>
    <%= render 'shared/show', :all_types_object => all_types_object %>
  <% end %>
<% end %>

<%= link_to 'Back', read_test_relation_condition_option_form_path(encode_method(params[:apply_method]), params[:argument_type], params[:argument_type_option]) %>